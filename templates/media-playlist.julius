function load_playlist() {
    document.getElementById("#{idPlaylist}").innerHTML =
    "<span>n items</span>";
    save_playlist();
}
function save_playlist() {
    $.post( "@{rout}"
             , '{"name":[0,1,2]}'
             , function success(d, s, x){}
             );
}

$("##{idPlaylist}").ready(load_playlist)
